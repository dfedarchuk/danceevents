{% if items|length > 0 %}
    <div class="list-columns">
        <ul class="list-unstyled">
            {% for item in items -%}
                <li>
                    <a href="{{ item.custom == 'y' ? item.link : path(item.link) }}">
                        {{ item.label }}
                    </a>
                </li>
            {% endfor -%}
        </ul>
    </div>
{% endif %}
