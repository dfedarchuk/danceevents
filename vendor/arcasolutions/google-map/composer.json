{
    "name": "arcasolutions/google-map",
    "description": "Google Map API v3 integration for PHP 5.3+",
    "keywords": [ "google map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Mosela",
            "email": "diego.mosela@arcasolutions.com",
            "role": "Developer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "egeloen/json-builder": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "widop/http-adapter": "~1.1.0",
        "willdurand/geocoder": "~2.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggest": {
        "widop/http-adapter": "Allows to use services",
        "willdurand/geocoder": "Allows to use geocoding service"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
